While (!userIn.equalsIgnoreCase(“Q”)) { If (userIn.equals(“1”)) {
intakeNewDog(scnr); userIn = scnr.nextLine():
}
If (userIn.equals(“2”)) {
intakeNewMonkey(scnr); userIn = scnr.nextLine();
}
if (userIn.equals(“3”)) {
reserveAnimal(scnr); userIn = scnr.nextLine();
}
if (userIn.equals(“4”)) { printAnimals(“dog”); userIn = scnr.nextLine();
}
if (userIn.equals(“5”)) {
printAnimals(“monkey”); userIn = scnr.nextLine();
}
if (userIn.equals(“6”))

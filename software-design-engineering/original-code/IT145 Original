}

 
else {
 

Systen.out.println(“Invalid input”) displayMenu();
userIn = scnr.nextLine();
 
}

}


If (userIn.equalsIgnoreCase(“q”)) { System.out.printzln(“Exiting program”); Scnr.close.();
System.exit(0);
}

//Constructor
Public Monkey(String name, String gender, String age,
String weight, String acquisitionDate, String acquisitionCountry, String trainingStatus, Boolean reserved, String inServiceCountry, String tailLength, String height, String bodyLength;
Private String species;


//Available monkey species
String[] monkeySpecies = {“Capuchin”, “Guenon”, “Macaque”, “Marmoset”, “Squirrel Monkey”, “Tamarin”};
//Constructor
Public Monkey(String name, String gender, String age,
String weight, String acquisitionDate, String acquistionCountry, String trainingStatus, Boolean reserved, String inServiceCountry, String tailLength, String height, String bodyLength, String species) {
setName(name); setGender(gender); setAge(age); setWeight(weight);
setAcquisitionDate(acquisitionDate); setAcquisitionLocation(acquisitionCountry); setTrainingStatus(trainingStatus); setReserved(reserved); setInServiceCountry(inServiceCountry); setTailLength(tailLength).
 
setHeight(height); setBodyLength(bodyLength); setSpecies(species);
}
//Accessor methods
Public String getTailLength() { return tailLength;
}
Public String getHeight() {
return height;
}
Public String getBodyLength() { Return bodyLength;
}
Public String getSpecies() {
}
Public void setHeight(String monkeyTailLength) { tailLength = monkeyTailLength;
}
Public void setHeight(String monkeyHeight) { tailLength = monkeyTailLength;
}
Public void setBodyLength(String monkeyHeight) { tailLength = monkeyHeight;
}
Public void setSpecies(String monkeySpecies) { bodyLength = monkeyBodyLength;
}
public void setSpecies(String monkeySpecies) {
 
species = monkeySpecies);

